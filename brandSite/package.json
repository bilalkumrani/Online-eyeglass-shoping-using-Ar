{
  "name": "ecommerce-app",
  "version": "1.0.0",
  "description": "It is a Ecommerce website with admin dashboard using MERN",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon back-end/src/app.js",
    "client": "npm start --prefix front-end",
    "install-server": "npm i --prefix back-end",
    "install-client": "npm i --prefix front-end",
    "install-project": "concurrently \"npm run install-server\" \"npm run install-client\"",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Daniyal-Shah/React-Dashboard.git"
  },
  "author": "Daniyal Shah",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Daniyal-Shah/React-Dashboard/issues"
  },
  "homepage": "https://github.com/Daniyal-Shah/React-Dashboard#readme",
  "dependencies": {
    "concurrently": "^7.0.0"
  }
}
